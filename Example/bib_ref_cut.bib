@Conference{Yamauchi-01-94,
  Title                    = {Heat generation around fracturing point during the tearing process of paper},
  Author                   = {Yamauchi, T. and Tanaka, A.},
  Year                     = {2001},

  Address                  = {Tokyo},
  Pages                    = {94-97},

  _language                = {Japanese},
  _note                    = {cited By (since 1996)1; Conference of org.apache.xalan.xsltc.dom.DOMAdapter@3b60cc09 ; Conference Date: org.apache.xalan.xsltc.dom.DOMAdapter@4ff38153 Through org.apache.xalan.xsltc.dom.DOMAdapter@7baf9926; Conference Code:58739},
  Abbrev_source_title      = {Proc Pulp Pap Res Conf},
  Abstract                 = {Temperature distribution of paper under tearing process was measured by infrared thermography. Tearing was performed by tensile tester and was able to simulate the Elmendorf tearing test. Infrared thermography successfully allowed the successive and detailed observation of heat generation around the crack tip. The stressed zone, which appears as high temperature zone, is larger in softwood pulp sheets than in hardwood pulp sheets. With the increase in beating level, both the maximum temperature-rise and the area of high temperature zone change in the same way as tear strength. They increase monotonously in the case of hardwood pulp sheet, while they decrease after passing an early peak in the case of softwood pulp sheet. The decrease in the area refers to an enhanced stress concentration. The increase in the maximum temperature rise caused by lightly beating refers to a large amount of work consumed at the crack tip. It also includes the work for fiber pulling-out.},
  Affiliation              = {KCL Science and Consulting, P.O. Box 70, Espoo, Finland},
